   "source": [
    "#IP 확인 API로 접근해서 결과 출력하기\n",
    "#모듈 읽어 들이기\n",
    "import urllib.request\n",
    "\n",
    "#데이터 읽어 들이기\n",
    "url = \"http://api.aoikujira.com/ip/ini\"\n",
    "res = urllib.request.urlopen(url)\n",
    "data = res.read()\n",
    "\n",
    "#바이너리를 문자열로 변환하기\n",
    "text = data.decode(\"utf-8\")\n",
    "print(text)"
   ]
  }
